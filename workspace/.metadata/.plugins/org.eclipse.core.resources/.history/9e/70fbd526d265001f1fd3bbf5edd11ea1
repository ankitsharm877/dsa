package demo;

import java.util.Map;
import java.util.function.Function;
import java.util.stream.Collectors;

public class Demo {
	
	public static void main(String[] args) {
		
		String str = "dhnd^^  &&&&&   $$$3288e";
		// count of special character
	
		Map<Integer, Long> collect = str.chars().filter(ch -> !Character.isAlphabetic(ch) && !Character.isDigit(ch))
		.boxed().collect(Collectors.groupingBy(Function.identity(), Collectors.counting()));
		System.out.println(collect);
	}
}